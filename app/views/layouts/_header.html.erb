<% cache "header" do -%>
    <table width="100%">
      <tr>
      <td style="text-align:left">
        <%= link_to image_tag("seek-logo-beta_smaller.png",:alt=>"SEEK",:title=>"SEEK"),
          root_path %>
      </td>
      <td style="width:90%;">
        <%= render :partial=>"layouts/header_text_main" -%>
      </td>
      <td style="text-align:right;">
        <%if defined? HEADER_IMAGE%><%= link_to image_tag(HEADER_IMAGE,:alt=>HEADER_IMAGE_LINK,:title=>HEADER_IMAGE_TITLE),
          HEADER_IMAGE_LINK %><%end%>
      </td>
    </tr>
    </table>      
<% end -%>
