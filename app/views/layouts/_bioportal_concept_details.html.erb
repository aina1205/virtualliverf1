
<div class="concept_details">
  <ul>
    <li>
      <label><b>Synonyms (<%= concept[:synonyms].count -%>) </b></label>
      <%= concept[:synonyms].join(", ") %>
    </li>
    <li>
      <label><b>Definitions (<%= concept[:definitions].count -%>) </b></label>
      <%= concept[:definitions].join(", ") %>
    </li>
    <li>
      <%= link_to "Visualise on BioPortal",visualise_organism_path(@organism),:popup=>true %>
    </li>
    <li>
      <%= link_to_ncbi_taxonomy_browser @organism,"Link to NCBI taxonomy browser",:popup=>true %>
    </li>
  </ul>
</div>