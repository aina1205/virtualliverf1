<h3> <%= h(@experiment.title) %> </h3>

<br/>


<% for sop in @experiment.sops %>

  <br/>
  <%= "Sop title=#{sop.title}" %>

<% end %>
<br/>

<%= "Topic: #{@experiment.topic.title}" if @experiment.topic %>

<br/>
<% for assay in @experiment.assays %>
  <%= "Assay title=#{assay.title}" %>

<% end %>

<br/>
<%= link_to "Edit",edit_experiment_path(@experiment) %>