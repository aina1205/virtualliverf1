<%= list_item_title resource %>

<%= list_item_attribute "PubMed ID", resource.pubmed_id, "http://www.ncbi.nlm.nih.gov/pubmed/#{resource.pubmed_id}"%>

<%= list_item_simple_list resource.asset.creators, "Author" -%>

<%= list_item_expandable_text "Abstract", h(resource.abstract) %>

<%= list_item_optional_attribute "Journal", h(resource.journal) %>

<%= list_item_attribute "Date Published", resource.published_date.strftime('%d/%m/%Y') %>     
