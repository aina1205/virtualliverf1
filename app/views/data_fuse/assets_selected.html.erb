<div class="show_basic">

  DataFile:<br/>
  <%= render :partial => "layouts/resource_list_item",:object=>@data_file -%>
  <br/>

  Model:<br/>
  <%= render :partial => "layouts/resource_list_item",:object=>@model -%>
  <br/>

  Symbols you selected that were not found in the data:
  <b><%= (@parameter_keys - @matched_keys).sort.join(", ") -%></b>
  <br/>

  Parameters summary:<br/>
  <%= render :partial=>"csv_view",:locals=>{:csv=>@matching_csv,:highlight_first_row=>true} -%>
  <br/>

  <% form_tag({:action=>:submit},:method=>:post) do -%>
    <%= hidden_field_tag :data_file_id,@data_file.id -%>
    <%= hidden_field_tag :model_id,@model.id -%>
    <%= hidden_field_tag :parameter_keys,@parameter_keys -%>
    <%= hidden_field_tag :matching_keys,@matching_keys -%>
    <%= hidden_field_tag :parameter_csv,@matching_csv -%>

    <%= submit_tag "Go!",:disable_with=>"Sending..." -%>
  <% end -%>

</div>