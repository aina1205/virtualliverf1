<h3><%= "Page #{pagenum.to_s}  of #{pages.to_s}" -%></h3>
<table>
  <tr><th>Ontology label</th><th>Ontology ID</th><th>Version ID</th><th>Concept id</th><th>Preferred name</th></tr>
  <% search_results.each do |result| %>
  <tr>
    <td><%= h(result[:ontology_display_label]) -%></td>
    <td><%= link_to_ontology(result[:ontology_id],result[:ontologyId]) -%></td>
    <td><%= link_to_ontology_version(result[:ontology_version_id],result[:ontology_version_id]) -%>
        <%= link_to_ontology_version_visualize("Explore",result[:ontology_version_id]) -%>
      <%= link_to_download_ontology("Download",result[:ontology_version_id]) -%>
    </td>
    <td>
      <%= result[:concept_id] -%>
    </td>
    <td>
      <%= link_to_function(h(result[:preferred_name]),"add_concept(#{result[:ontology_id]},#{result[:ontology_version_id]},'#{result[:concept_id]}');") -%>
    </td
  </tr>
  <% end %>

</table>
